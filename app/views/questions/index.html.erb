<% @questions.each do |question| %>
    <div>
      <h1>title:</h1>
      <%= question.title %>
      <h1>body:</h1>
      <%= question.body %>

    </div>
    <%= link_to "Show", question_path(question)%>

    <%= link_to "Edit", edit_question_path(question)%>
    <%= link_to "Delete", question_path(question),method: :delete %>

    <hr>

<% end %>